<Page x:Class="TestApplicationWPF.Pages.TestResult"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:TestApplicationWPF.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="560" d:DesignWidth="1050"
      Margin="50 0 0 0"
      Title="TestResult">

    <Grid Background="#ecf0f1">

        <StackPanel>
            <Label>Correct</Label>
            <TextBox Name="Correct" ></TextBox>
            <Label>False</Label>
            <TextBox Name="Wrong" ></TextBox>
            <Label>NotAnswered</Label>
            <TextBox Name="NotAnswer"></TextBox>
        </StackPanel>
    </Grid>
</Page>
